        "source.fixAll": true
    },
    "dart.lineLength": 100,
    "[dart]": {
        "editor.rulers": [100]
    },
    "files.exclude": {
        ".dart_tool/": true,
        ".flutter-plugins*": true,
        ".idea/": true,
        ".metadata": true,
        ".packages": true,
        "*.iml": true,
        "pubspec.lock": true
    }
}